alias filetree="find | sed 's|[^/]*/|- |g'"
alias dirtree='find . -type d | sed -e "s/[^-][^\/]*\//  |/g" -e "s/|\([^ ]\)/|-\1/"'
alias lst='ls -R | grep ":$" | sed -e '"'"'s/:$//'"'"' -e '"'"'s/[^-][^\/]*\//--/g'"'"' -e '"'"'s/^/   /'"'"' -e '"'"'s/-/|/'"'"
alias hi='cat /var/log/shell.log '
alias adapterlist='esxcli storage core adapter list'
alias dischi='cd /vmfs/volumes/ && ll'
alias fwon='esxcli network firewall set --enabled true'
alias addiscsi='esxcli iscsi adapter discovery statictarget  add -A '
alias partition='esxcli storage core device partition list'
alias showrunvm='esxcli vm process list'
alias montavol='esxcfg-volume -M '
alias modlist='esxcli system module list'
alias bootdevice='esxcli system boot device get'
alias trovaf='find . -type f -name '
alias ifconfig='esxcli network ip interface ipv4 get'
alias volumi='esxcli storage filesystem list'
alias locale1='cd /vmfs/volumes/datastore1/ && ll'
alias fwoff='esxcli network firewall set --enabled false'
alias vmstorage='cd /vmfs/volumes/vmstorage/ && ll'
alias raid1='cd /vmfs/volumes/raid/ && ll'
alias killvm='esxcli vm process kill -t hard -w '
alias listavol='esxcfg-volume -l | grep label'
alias ll='/bin/ls -alhtrFA'
alias trova='grep -inHrv -e "#"-f /'
alias vmfile='cd /vmfs/volumes/vmfile/ && ll'
alias netstat='esxcli network ip connection list'
alias nfsmount='esxcli storage nfs list'
alias addnfs=' esxcli storage nfs add -H 10.0.0.29 -s /volume2/vmfile -v vmfilenfs'
alias showrunvmx=' showrunvm | grep Config| cut -c 17-'
alias lista-volumi="find  /vmfs/volumes/ -type l  -maxdepth 1"
alias enable-mob="vim-cmd hostsvc/advopt/update Config.HostAgent.plugins.solo.enableMob bool true"
alias test-mob="vim-cmd hostsvc/advopt/view Config.HostAgent.plugins.solo.enableMob"
